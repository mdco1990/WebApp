{
  "name": "webapp-ui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview --port 5173",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 25",
    "lint:fix": "eslint . --fix",
    "lint:css": "stylelint 'src/**/*.{css,scss}'",
    "lint:css:fix": "stylelint 'src/**/*.{css,scss}' --fix",
    "lint:strict": "eslint . --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "lint:all": "npm run lint && npm run lint:css"
  },
  "dependencies": {
    "bootstrap": "5.3.3",
    "bootstrap-icons": "^1.13.1",
    "chart.js": "^4.5.0",
    "i18next": "^23.12.2",
    "react": "18.2.0",
    "react-bootstrap": "^2.10.4",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "18.2.0",
    "react-i18next": "^13.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.2.0",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.33.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "stylelint": "^16.23.1",
    "stylelint-config-recommended-scss": "^16.0.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-order": "^6.0.4",
    "terser": "^5.43.1",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.39.0",
    "vite": "^6.3.5",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-stylelint": "^6.0.2"
  }
}
